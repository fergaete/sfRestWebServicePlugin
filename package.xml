<?xml version='1.0' encoding='UTF-8' ?>
<package xmlns="http://pear.php.net/dtd/package-2.0" xmlns:tasks="http://pear.php.net/dtd/tasks-1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" packagerversion="1.4.1" version="2.0" xsi:schemaLocation="http://pear.php.net/dtd/tasks-1.0 http://pear.php.net/dtd/tasks-1.0.xsd http://pear.php.net/dtd/package-2.0 http://pear.php.net/dtd/package-2.0.xsd">
  <name>sfRestWebServicePlugin</name>
  <channel>plugins.symfony-project.org</channel>
  <summary>A REST webservices module generator</summary>
  <description>The sfRestWebServicePlugin offers an easy interface for REST API based on your domain model.</description>
  <lead>
    <name>Alessandro Nadalin</name>
    <user>odino</user>
    <email>alessandro.nadalin@gmail.com</email>
    <active>yes</active>
  </lead>
  <date>2010-09-06</date>
  <version>
    <release>0.9.0</release>
    <api>0.9.0</api>
  </version>
  <stability>
    <release>beta</release>
    <api>beta</api>
  </stability>
  <license uri="http://www.symfony-project.org/license">MIT license</license>
  <notes>
    * see plugin README on github
  </notes>
  <contents>
  <dir name="/">
    <file role="data" name="README" />
    <file role="data" name="LICENSE" />
    <file role="data" name="VERSION" />

    <dir name="config">
      <file role="data" name="config.yml" />
      <file role="data" name="routing.yml" />
      <file role="data" name="sfRestWebServicePluginConfiguration.class.php" />
    </dir>

    <dir name="lib">
      <file role="data" name="sfRestWebServiceConfiguration.class.php" />
    </dir>

    <dir name="modules">
      <dir name='sfRestWebService'>
        <dir name="actions">
          <file role="data" name="actions.class.php" />
        </dir>
        <dir name="templates">
          <file role="data" name="deleteSuccess.json.php" />
          <file role="data" name="deleteSuccess.xml.php" />
          <file role="data" name="deleteSuccess.yaml.php" />
          <file role="data" name="entrySuccess.json.php" />
          <file role="data" name="entrySuccess.xml.php" />
          <file role="data" name="entrySuccess.yaml.php" />
          <file role="data" name="errorSuccess.json.php" />
          <file role="data" name="errorSuccess.xml.php" />
          <file role="data" name="errorSuccess.yaml.php" />
          <file role="data" name="objectSuccess.json.php" />
          <file role="data" name="objectSuccess.xml.php" />
          <file role="data" name="objectSuccess.yaml.php" />
          <file role="data" name="resourceSuccess.xml.php" />
        </dir>
      </dir>
    </dir>
  </dir>
  </contents>
  <dependencies>
    <required>
      <php>
        <min>5.2.4</min>
      </php>
      <pearinstaller>
        <min>1.4.1</min>
      </pearinstaller>
      <package>
        <name>symfony</name>
        <channel>pear.symfony-project.com</channel>
        <min>1.4.0</min>
        <max>1.5.0</max>
        <exclude>1.5.0</exclude>
      </package>
    </required>
  </dependencies>
  <phprelease></phprelease>
  <changelog>
    <release>
      <version>
        <release>0.9.0</release>
        <api>0.9.0</api>
      </version>
      <stability>
        <release>beta</release>
        <api>beta</api>
      </stability>
      <license uri="http://www.symfony-project.com/license">
        MIT license
      </license>
      <date>2010-09-06</date>
      <license>MIT</license>
      <notes>
         * odino: first release
      </notes>
    </release>
  </changelog>
</package>